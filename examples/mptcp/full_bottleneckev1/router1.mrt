
ifconfig:

# ethernet card (modelled by point-to-point link)  -> mptcp 1.0 10.1.1.1
name: ppp0  inet_addr: 10.1.1.2 MTU: 1500   Metric: 1  POINTTOPOINT MULTICAST

# ethernet card (modelled by point-to-point link) -> Router 2.0 10.2.4.1
name: ppp1  inet_addr: 10.2.5.2 MTU: 1500   Metric: 1  POINTTOPOINT MULTICAST

# ethernet card (modelled by point-to-point link) -> Default 1.0  10.100.0.1
name: ppp2  inet_addr: 10.100.0.2 MTU: 1500   Metric: 1  POINTTOPOINT MULTICAST

ifconfigend.

route:
10.1.1.1     	10.1.1.1		255.255.255.255		H	0	ppp0
10.100.0.1     	10.100.0.1		255.255.255.255		H	0	ppp2
0.0.0.0		    10.2.5.1		0.0.0.0			    G	0	ppp1
 
routeend.